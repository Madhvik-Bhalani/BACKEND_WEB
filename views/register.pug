    <div id="form">
   
      <form action="/register" method="post">
        <h1>registration form</h1>
        <input type="text" name="name" id="name" placeholder="Enter Name*" />
        <input type="number" name="mno" id="mno" placeholder="Enter Mobile_No*" />
        <input type="email" name="mail" id="mail" placeholder="Enter Mail*" />
        <input type="number" name="age" id="age" placeholder="Enter Your Age*">
        <input type="number" name="std" id="std" placeholder="Enter Your Std*">
        <div id="btns">

          <input class="btn" id="btn1" type="submit" value="submit" />
          <input class="btn" id="btn2" type="reset" value="reset now">
        </div>
      </form>
    </div>

    <footer>
      <h1>all rights reserved&copy;</h1>
    </footer>
